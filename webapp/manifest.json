{
  "_version": "1.76.0",
  "sap.app": {
    "id": "geonosis.project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.6",
      "toolsId": "464760e9-c79c-4ba8-9476-74dde05dfb73"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "geonosis.project1.i18n.i18n"
        }
      },
      "data": {
          "type": "sap.ui.model.json.JSONModel",
          "uri": "model/Data.json"
      }

    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "geonosis.project1.view",
        "controlId": "flexId",
        "controlAggregation": "beginColumnPages",
        "async": true
      },
      "routes": [
        {
          "name": "begin",
          "pattern": "",
          "target": ["begin"]
        },
        {
          "name": "detail",
          "pattern": "detail/{id}",
          "target": ["begin", "detail"]
        }
      ],
      "targets": {
        "begin": {
          "viewName": "View1",
          "viewId": "View1",
          "transition":"slide",
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "viewName": "Detail",
          "viewId": "Detail",
          "transition":"slide",
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "geonosis.project1.view.App",
      "type": "XML",
      "id": "app",
      "async": true
    }
  }
}
